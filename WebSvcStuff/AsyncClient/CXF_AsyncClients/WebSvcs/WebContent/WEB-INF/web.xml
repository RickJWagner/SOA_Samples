<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>WebSvcs</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <display-name>Lowercaser</display-name>
    <servlet-name>Lowercaser</servlet-name>
    <servlet-class>org.example.Lowercaser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Lowercaser</servlet-name>
    <url-pattern>/Lowercaser</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>Uppercaser</display-name>
    <servlet-name>Uppercaser</servlet-name>
    <servlet-class>org.example.Uppercaser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Uppercaser</servlet-name>
    <url-pattern>/Uppercaser</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>GetOne</display-name>
    <servlet-name>GetOne</servlet-name>
    <servlet-class>org.example.GetOne</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetOne</servlet-name>
    <url-pattern>/GetOne</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>GetTwo</display-name>
    <servlet-name>GetTwo</servlet-name>
    <servlet-class>org.example.secondnamespace.GetTwo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetTwo</servlet-name>
    <url-pattern>/GetTwo</url-pattern>
  </servlet-mapping>
</web-app>